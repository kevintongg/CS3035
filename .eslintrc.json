// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "no-console": "off",
    "no-mixed-operators": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-alert": "off",
    "no-unused-vars": "warn"
  },
  "comma-dangle": 0
}
